{
  "presets": [
    ["@babel/env", { "loose": true, "modules": false }],
    "@babel/stage-3",
    "@babel/react"
  ],
  "plugins": ["@babel/plugin-proposal-class-properties"],
  "env": {
    "production": {
      "plugins": [
        [
          "transform-react-remove-prop-types",
          {
            "removeImport": true
          }
        ]
      ]
    }
  }
}
